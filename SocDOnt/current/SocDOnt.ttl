###  https://w3id.org/SocDOnt/1.0#label
:label rdf:type owl:DatatypeProperty ,
                owl:FunctionalProperty ;
       rdfs:domain :Tag ;
       rdfs:range xsd:string .


###  https://w3id.org/SocDOnt/1.0#last_update
:last_update rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :IntellectualComponent ,
                         :IntellectualEntity ;
             rdfs:range xsd:dateTime .


###  https://w3id.org/SocDOnt/1.0#mentionees
:mentionees rdf:type owl:DatatypeProperty ;
            rdfs:domain :IntellectualComponent ,
                        :IntellectualEntity ;
            rdfs:range xsd:object .


###  https://w3id.org/SocDOnt/1.0#title
:title rdf:type owl:DatatypeProperty ,
                owl:FunctionalProperty ;
       rdfs:domain :IntellectualComponent ,
                   :IntellectualEntity ;
       rdfs:range xsd:string .


