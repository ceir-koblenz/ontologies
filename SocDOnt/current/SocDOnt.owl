    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>/1.0#BlogPost</IRI>
        <Literal xml:lang="en">BlogPosts belong to the IntellectualEntities and thus describe entries that are posted in blogs.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>/1.0#BlogPost</IRI>
